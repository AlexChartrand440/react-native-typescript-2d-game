{
    "expo": {
        "name": "The Box",
        "description": "A simple 2D game (called The Box) implemented in React Native, as an Expo app, using TypeScript.",
        "slug": "the-box",
        "githubUrl": "https://github.com/shirakaba/react-native-typescript-2d-game",
        "privacy": "public",
        "version": "1.8.2",
        "primaryColor": "#ff7f00",
        "icon": "./assets/icons/icon.png",
        "updates": {
            "enabled": true,
            "checkAutomatically": "ON_LOAD",
            "fallbackToCacheTimeout": 0
        },
        "sdkVersion": "26.0.0",
        "splash": {
            "image": "./assets/icons/loading.png",
            "resizeMode": "contain",
            "backgroundColor": "#FFFFFF",
            "hideExponentText": false
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "uk.co.bottledlogic.thebox.app",
            "buildNumber": "1.8.2",
            "icon": "./assets/icons/icon.png",
            "splash": {
                "image": "./assets/icons/loading.png",
                "tabletImage": "./assets/icons/loading.png",
                "resizeMode": "contain",
                "backgroundColor": "#FFFFFF",
                "hideExponentText": false
            }
        }
    }
}
